//program to find the index of a number in an array
//First approach
#include <iostream>

const int SIZE=100;
void readArray(int arr[], int n)
{
    for (int i=0; i<n; i++)
    {
        std::cin>>arr[i];
    }
}
int findIndexOfNumInArray(int *arr, int n, int num)
{
   for (int i=0; i<n; i++)
   {
       if (arr[i]==num)
       {
           return i;
       }
   }
   return -1;
}

int main()
{
    int arr[SIZE]={0};
    int n;
    std::cin>>n;
    readArray(arr,n);
    int num;
    std::cin>>num;

    std::cout<<findIndexOfNumInArray(arr,n,num);




    return 0;
}
